{
  "name": "influx-syslog",
  "version": "0.2.4",
  "description": "Node.js InfluxDB syslog logger",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/node-influx-syslog-logger",
  "license": "MIT",
  "scripts": {
    "start": "yarn spec:watch",
    "test": "yarn pretty && yarn lint && yarn spec",
    "spec": "CI=true jest --runInBand --colors",
    "spec:watch": "yarn spec --watch",
    "spec:coverage": "yarn spec --coverage",
    "lint": "eslint --ext .ts src/",
    "pretty": "prettier --check '{src,test}/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "build": "yarn build:source && yarn build:typings",
    "build:source": "babel --source-maps --extensions .js,.ts --out-dir lib/ --delete-dir-on-start --ignore **/__tests__ src/",
    "build:typings": "tsc --declaration",
    "build:watch": "yarn build --watch --verbose",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "influx": "^5.5.1",
    "lodash.debounce": "^4.0.8",
    "lodash.padstart": "^4.6.1",
    "strip-ansi": "^6.0.0",
    "through2": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/bunyan": "^1.8.6",
    "@types/jest": "^24.0.25",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.padstart": "^4.6.6",
    "@types/node": "^13.1.2",
    "@types/through2": "^2.0.34",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-plugin-module-name-mapper": "^1.2.0",
    "bunyan": "^1.8.12",
    "debug-utils": "^0.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  }
}
